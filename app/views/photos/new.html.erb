<!-- Page Heading/Breadcrumbs -->
<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Upload Photo of Monument</h1>
  </div>
</div>
<!-- /.row -->

<!-- Content Row -->
<div class="row">
  <div class="col-lg-12">
    <div class="row">
      <%= form_for [@collection, @monument, @photo], :html => { :multipart => true } do |f| %>
        <%= f.file_field :image %>
        <%= f.submit (@monument.new_record? ? "Create" : "Update"), :class => "btn btn-primary" %>

    <% end %>
    </div>
    <!-- /.row -->

  </div>
</div>
<!-- /.row -->


